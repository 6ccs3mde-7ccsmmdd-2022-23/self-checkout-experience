<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="selfCheckoutExperience" nsURI="http://www.SelfCheckoutExperience.self_checkout_experience"
    nsPrefix="selfCheckoutExperience">
  <eClassifiers xsi:type="ecore:EClass" name="Self_checkout">
    <eStructuralFeatures xsi:type="ecore:EReference" name="pickScanMachine" eType="#//HoldSelfScanner"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="pickstatements" upperBound="-1"
        eType="#//PickStatement" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="walkstatements" upperBound="-1"
        eType="#//WalkStatement" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="checkout" eType="#//Checkout"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="HoldSelfScanner">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="scanMachine" eType="#//SelfScanner"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="SelfScanner">
    <eLiterals name="selfscanner" literal="selfscanner"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="PickStatement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="itemCount" eType="#//Expression"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="itemPicked" eType="#//ItemDef"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="holdingItem" eType="ecore:EClass http://www.eclipse.org/emf/2002/Ecore#//EObject"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ItemDef">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ScanAndAddToBasket">
    <eStructuralFeatures xsi:type="ecore:EReference" name="itemInBasket" eType="#//ItemDef"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Drop">
    <eStructuralFeatures xsi:type="ecore:EReference" name="itemDropped" eType="#//ItemDef"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="WalkStatement"/>
  <eClassifiers xsi:type="ecore:EClass" name="VariableDeclaration" eSuperTypes="#//WalkStatement">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Repeat" eSuperTypes="#//WalkStatement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="count" eType="#//Expression"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="statements" upperBound="-1"
        eType="ecore:EClass http://www.eclipse.org/emf/2002/Ecore#//EObject" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="MoveStatement" eSuperTypes="#//WalkStatement">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="command" eType="#//MoveCommand"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="steps" eType="#//Expression"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="MoveCommand">
    <eLiterals name="forward" literal="forward"/>
    <eLiterals name="backward" value="1" literal="backward"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="TurnStatement" eSuperTypes="#//WalkStatement">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="command" eType="#//TurnCommand"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="TurnCommand">
    <eLiterals name="left" literal="left"/>
    <eLiterals name="right" value="1" literal="right"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Expression"/>
  <eClassifiers xsi:type="ecore:EClass" name="IntLiteral" eSuperTypes="#//Expression">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="val" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="IntVarExpression" eSuperTypes="#//Expression">
    <eStructuralFeatures xsi:type="ecore:EReference" name="var" eType="#//VariableDeclaration"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Checkout">
    <eStructuralFeatures xsi:type="ecore:EReference" name="scan" eType="#//Scan" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="pay" eType="#//Pay"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Scan">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="scanMachine" eType="#//SelfScanner"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="addToBag" eType="#//AddToBag"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="AddToBag">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="bag" eType="#//Bag"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="Bag">
    <eLiterals name="bag" literal="bag"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="Pay">
    <eLiterals name="pay" literal="pay"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Addition" eSuperTypes="#//Expression">
    <eStructuralFeatures xsi:type="ecore:EReference" name="left" eType="#//Expression"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="operator" unique="false"
        upperBound="-1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="right" upperBound="-1"
        eType="#//Expression" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Multiplication" eSuperTypes="#//Expression">
    <eStructuralFeatures xsi:type="ecore:EReference" name="left" eType="#//Expression"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="operator" unique="false"
        upperBound="-1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="right" upperBound="-1"
        eType="#//Expression" containment="true"/>
  </eClassifiers>
</ecore:EPackage>
